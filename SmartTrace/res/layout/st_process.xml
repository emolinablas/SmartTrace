<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" android:background="@drawable/background1">
    
        <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" android:gravity="center">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:layout_marginTop="10sp"
                android:background="@drawable/sincho"
                android:gravity="left|center"
                android:paddingLeft="10sp"
                android:text="PROCESAR BARRILES"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="bold" />
        
        <LinearLayout 
            android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	android:orientation="horizontal"
        	android:gravity="top|center">
        	
            <LinearLayout 
                android:layout_width="wrap_content"
        		android:layout_height="wrap_content"
        		android:orientation="vertical"
        		android:gravity="center">
                
                <ImageButton
                android:id="@+id/process_armar_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/armar" />
                
                <TextView 
                    android:id="@+id/tv001"
                    android:layout_width="120sp"
                    android:layout_height="wrap_content"
                    android:text="Armar" android:gravity="center" android:textColor="#000000" android:textStyle="bold"/>"
                
            </LinearLayout>
            
            <LinearLayout 
                android:layout_width="wrap_content"
        		android:layout_height="wrap_content"
        		android:orientation="vertical"
        		android:gravity="center">
                
                <ImageButton
                android:id="@+id/process_reparar_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/reparar" />
                
                <TextView 
                    android:id="@+id/tv001"
                    android:layout_width="120sp"
                    android:layout_height="match_parent"
                    android:text="Reparar" android:gravity="center" android:textColor="#000000" android:textStyle="bold"/>
                
            </LinearLayout>
        </LinearLayout>
        
        
        <LinearLayout 
            android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:orientation="horizontal"
        	android:gravity="center">
        	
            <LinearLayout 
                android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:orientation="vertical"
        		android:gravity="center">
                
                <ImageButton
                android:id="@+id/process_raspar_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/raspar" />
                
                <TextView 
                    android:id="@+id/tv001"
                    android:layout_width="120sp"
                    android:layout_height="wrap_content"
                    android:text="Raspar" android:gravity="center" android:textColor="#000000" android:textStyle="bold"/>
                
            </LinearLayout>
            
            <LinearLayout 
                android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:orientation="vertical"
        		android:gravity="center">
                
                <ImageButton
                android:id="@+id/process_quemar_empanzar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/quemado" />
                
                <TextView 
                    android:id="@+id/tv001"
                    android:layout_width="120sp"
                    android:layout_height="wrap_content"
                    android:text="Quemar" android:gravity="center" android:textColor="#000000" android:textStyle="bold"/>
                
            </LinearLayout>
        </LinearLayout>
        
        
        <LinearLayout 
            android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:orientation="horizontal"
        	android:gravity="center">
        	
            <LinearLayout 
                android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:orientation="vertical"
        		android:gravity="center">
                
                <ImageButton
                android:id="@+id/process_empanzar_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/empanzar" />
                
                <TextView 
                    android:id="@+id/tv001"
                    android:layout_width="120sp"
                    android:layout_height="wrap_content"
                    android:text="Empanzar" android:gravity="center" android:textColor="#000000" android:textStyle="bold"/>
                
            </LinearLayout>
            
            <LinearLayout 
                android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:orientation="vertical"
        		android:gravity="center">
                
                <ImageButton
                android:id="@+id/process_llenar_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/llenar" />
                
                <TextView 
                    android:id="@+id/tv001"
                    android:layout_width="120sp"
                    android:layout_height="wrap_content"
                    android:text="Llenar" android:gravity="center" android:textColor="#000000" android:textStyle="bold"/>
                
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    

</LinearLayout>